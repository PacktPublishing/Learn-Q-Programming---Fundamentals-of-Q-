<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>
  
  <!--
      We suppress the "unreachable code" warning, as a known issue in the Q# compiler
      can cause unreachable code to be emitted.
  -->
  <PropertyGroup>
    <NoWarn>0162</NoWarn>
  </PropertyGroup>
    
  <ItemGroup>
    <PackageReference Include="Microsoft.Quantum.Development.Kit" Version="0.3.1811.203-preview" />
    <PackageReference Include="Microsoft.Quantum.Canon" Version="0.3.1811.203-preview" />
  </ItemGroup>
</Project>
